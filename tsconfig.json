{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "./src/components/*"
      ],
      "modules/*": [
        "./src/modules/*"
      ],
      "navigations/*": [
        "./src/navigations/*"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "redux/*": [
        "./src/redux/*"
      ],
      "localization/*": [
        "./src/localization/*"
      ],
      "typings/*": [
        "./src/typings/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "theme/*": [
        "./src/theme/*"
      ]
    }
  }
}
